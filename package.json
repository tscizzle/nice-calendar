{
  "name": "nice-calendar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.21",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-icons": "^3.0.5",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.1",
    "redux-multi": "^0.1.12",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  },
  "scripts": {
    "wipe-css": "rm -r src/stylesheets ||:",
    "build-css": "node-sass src/ -o src/stylesheets",
    "watch-css": "node-sass src/ -o src/stylesheets --watch --recursive",
    "start-react": "BROWSER=none react-scripts start",
    "start": "npm-run-all -p wipe-css build-css watch-css start-react",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
