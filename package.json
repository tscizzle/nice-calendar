{
  "name": "nice-calendar",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "11.2.0",
    "npm": "6.4.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.21",
    "mongoose": "^5.12.14",
    "morgan": "^1.9.1",
    "node-sass": "^6.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "passport.socketio": "^3.7.0",
    "rc-time-picker": "^3.6.3",
    "react": "^16.6.3",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^4.0.3",
    "redis": "^3.1.1",
    "redux": "^4.0.1",
    "redux-multi": "^0.1.12",
    "redux-thunk": "^2.3.0",
    "rollbar": "^2.5.0",
    "sendgrid": "^5.2.3",
    "socket.io": "^2.4.1",
    "socket.io-client": "^4.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.3"
  },
  "scripts": {
    "wipe-css": "rm -r src/stylesheets ||:",
    "build-css": "node-sass src/ -o src/stylesheets",
    "watch-css": "node-sass src/ -o src/stylesheets --watch --recursive",
    "start-react": "BROWSER=none react-scripts start",
    "//": "in development, use these 4 commands (client, mongo, redis, server)",
    "start-client-dev": "npm-run-all -p wipe-css build-css watch-css start-react",
    "start-mongo-dev:macos": "mongod",
    "start-mongo-dev:linux": "sudo service mongod start",
    "stop-mongo-dev:linux": "sudo service mongod stop",
    "start-redis-dev": "redis-server",
    "start-server-dev": "nodemon server",
    "start": "node server",
    "build": "npm run build-css && react-scripts build",
    "heroku-postbuild": "npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
